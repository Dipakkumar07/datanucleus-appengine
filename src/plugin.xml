<plugin id="org.datanucleus.store.appengine" name="DataNucleus App Engine" provider-name="DataNucleus">
    <!-- STORE MANAGER -->
    <extension point="org.datanucleus.store_manager">
        <store-manager class-name="org.datanucleus.store.appengine.DatastoreManager" key="appengine" url-key="appengine"/>
    </extension>

    <!-- QUERY LANGUAGES -->
    <extension point="org.datanucleus.store_query_query">
        <query class-name="org.datanucleus.store.appengine.query.JDOQLQuery" datastore="appengine" name="javax.jdo.query.JDOQL"/>
        <query class-name="org.datanucleus.store.appengine.query.JDOQLQuery" datastore="appengine" name="JDOQL"/>
        <query class-name="org.datanucleus.store.appengine.query.JPQLQuery" datastore="appengine" name="javax.jdo.query.JPQL"/>
        <query class-name="org.datanucleus.store.appengine.query.JPQLQuery" datastore="appengine" name="JPQL"/>
    </extension>

    <!-- CONNECTION FACTORY -->
    <extension point="org.datanucleus.store_connectionfactory">
        <connectionfactory name="appengine" class-name="org.datanucleus.store.appengine.DatastoreConnectionFactoryImpl" transactional="true"
            datastore="appengine"/>
    </extension>

    <!-- IDENTIFIER FACTORIES -->
    <extension point="org.datanucleus.store_identifierfactory">
      <identifierfactory name="jpa" class-name="org.datanucleus.store.rdbms.sqlidentifier.JPAIdentifierFactory"/>
      <identifierfactory name="jpox" class-name="org.datanucleus.store.rdbms.sqlidentifier.JPOXIdentifierFactory"/>
      <identifierfactory name="jpox2" class-name="org.datanucleus.store.rdbms.sqlidentifier.JPOX2IdentifierFactory"/>
    </extension>
</plugin>
